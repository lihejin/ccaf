<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ccaf.dao.Tgen408Dao">

	<select id="get" resultType="com.ccaf.domain.Tgen408DO">
		select `bin_nbr`,`bch_cde`,`seq_nbr`,`atv_ind`,`map_bch_cde`,`lst_pan_seq_nbr`,`lst_rnm_bse_nbr`,`usd_pan_rnm_nbr`,`pdt_idn_cde`,`lst_upd_ttp`,`lst_upd_uid`,`lst_upd_nbr` from tb_tgen408 where bin_nbr = #{value}
