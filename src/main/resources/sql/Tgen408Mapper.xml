			<if test="usdPanRnmNbr != null">`usd_pan_rnm_nbr` = #{usdPanRnmNbr}, </if>
			<if test="pdtIdnCde != null">`pdt_idn_cde` = #{pdtIdnCde}, </if>
			<if test="lstUpdTtp != null">`lst_upd_ttp` = #{lstUpdTtp}, </if>
			<if test="lstUpdUid != null">`lst_upd_uid` = #{lstUpdUid}, </if>
			<if test="lstUpdNbr != null">`lst_upd_nbr` = #{lstUpdNbr}</if>
		</set>
		where bin_nbr = #{binNbr}
	</update>
	
	<delete id="remove">
		delete from tb_tgen408 where bin_nbr = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from tb_tgen408 where bin_nbr in 
		<foreach item="binNbr" collection="array" open="(" separator="," close=")">
