        <where>  
		  		  <if test="binNbr != null and binNbr != ''"> and bin_nbr = #{binNbr} </if>
		  		  <if test="bchCde != null and bchCde != ''"> and bch_cde = #{bchCde} </if>
		  		  <if test="seqNbr != null and seqNbr != ''"> and seq_nbr = #{seqNbr} </if>
