		  		  <if test="lstUpdUid != null and lstUpdUid != ''"> and lst_upd_uid = #{lstUpdUid} </if>
		  		  <if test="lstUpdNbr != null and lstUpdNbr != ''"> and lst_upd_nbr = #{lstUpdNbr} </if>
		  		</where>
	</select>
	 
	<insert id="save" parameterType="com.ccaf.domain.Tgen408DO">
		insert into tb_tgen408
		(
			`bin_nbr`, 
			`bch_cde`, 
			`seq_nbr`, 
			`atv_ind`, 
			`map_bch_cde`, 
			`lst_pan_seq_nbr`, 
			`lst_rnm_bse_nbr`, 
