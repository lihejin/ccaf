		  		  <if test="mapBchCde != null and mapBchCde != ''"> and map_bch_cde = #{mapBchCde} </if>
		  		  <if test="lstPanSeqNbr != null and lstPanSeqNbr != ''"> and lst_pan_seq_nbr = #{lstPanSeqNbr} </if>
		  		  <if test="lstRnmBseNbr != null and lstRnmBseNbr != ''"> and lst_rnm_bse_nbr = #{lstRnmBseNbr} </if>
		  		  <if test="usdPanRnmNbr != null and usdPanRnmNbr != ''"> and usd_pan_rnm_nbr = #{usdPanRnmNbr} </if>
